{
  "name": "ngx-keyboard-events",
  "version": "2.0.2",
  "description": "A simple [Angular](https://angular.io) module to bring an easy way to listen to keyboard events in a modularized way consistent with Angular's style.",
  "keywords": [
    "ngx",
    "angular",
    "keyboard events",
    "keyboard shorcuts",
    "keyboard"
  ],
  "peerDependencies": {
    "@angular/common": ">=13.2.0",
    "@angular/core": ">=13.2.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "module": "fesm2015/ngx-keyboard-events.mjs",
  "es2020": "fesm2020/ngx-keyboard-events.mjs",
  "esm2020": "esm2020/ngx-keyboard-events.mjs",
  "fesm2020": "fesm2020/ngx-keyboard-events.mjs",
  "fesm2015": "fesm2015/ngx-keyboard-events.mjs",
  "typings": "ngx-keyboard-events.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./ngx-keyboard-events.d.ts",
      "esm2020": "./esm2020/ngx-keyboard-events.mjs",
      "es2020": "./fesm2020/ngx-keyboard-events.mjs",
      "es2015": "./fesm2015/ngx-keyboard-events.mjs",
      "node": "./fesm2015/ngx-keyboard-events.mjs",
      "default": "./fesm2020/ngx-keyboard-events.mjs"
    }
  },
  "sideEffects": false
}